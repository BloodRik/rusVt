{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/layout.tsx", "app/components/Content/BtnApp.jsx", "app/components/Content/BtnAndroid.js", "app/components/Content/TitleInfoAndroid.js", "app/page.jsx", "app/components/Headers/Header.jsx", "app/components/Headers/NavOne.jsx", "app/components/Headers/NavTwo.jsx", "app/components/Headers/FonColorDarkLight.jsx", "app/components/Headers/ButtonVTB.jsx"],
  "exclude": ["node_modules"]
}
